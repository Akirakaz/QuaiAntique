<div class="mt-4 pb-1 border-b border-slate-100">
    <nav class="text-xs" aria-label="Breadcrumb">
        <ol class="flex list-none flex-wrap p-0">
            <li class="flex items-center">
                <a class="rounded p-2 hover:bg-slate-200" href="{{ path('app_home') }}">Accueil</a>
                <svg class="mx-2 h-3 w-3 fill-current" viewBox="0 0 320 512"><path d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"/></svg>
            </li>
            <li class="flex items-center">
                <a class="rounded p-2 hover:bg-slate-200" href="{{ path('app_admin_index') }}">Dashboard</a>
                <svg class="mx-2 h-3 w-3 fill-current" viewBox="0 0 320 512"><path d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"/></svg>
            </li>
            {% if categoryTitle is defined %}
                <li class="flex items-center">
                    <a class="rounded p-2 hover:bg-slate-200" href="{{ categorySlug }}">{{ categoryTitle }}</a>
                    <svg class="mx-3 w-3 h-3 fill-current" viewBox="0 0 320 512"><path d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"/></svg>
                </li>
                <li>
                    <span class="block p-2 text-gray-400" aria-current="page">{{ label }}</span>
                </li>
            {% else %}
            <li>
                <span class="block p-2 text-gray-400" aria-current="page">{{ label }}</span>
            </li>
            {% endif %}
        </ol>
    </nav>
</div>
